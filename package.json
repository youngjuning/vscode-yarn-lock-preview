{
  "name": "yarn-lock-preview",
  "displayName": "Yarn Lock Preview",
  "description": "Previews yarn.lock file",
  "version": "0.0.1",
  "icon": "assets/logo.png",
  "license": "MIT",
  "publisher": "youngjuning",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Visualization"
  ],
  "keywords": [
    "yarn",
    "preview"
  ],
  "activationEvents": [
    "onCommand:yarn-lock-preview.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "yarn-lock-preview.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "commit": "git cz"
  },
  "bugs": {
    "url": "https://github.com/vstirbu/vscode-mermaid-preview/issues"
  },
  "homepage": "https://github.com/vstirbu/vscode-mermaid-preview",
  "repository": {
    "type": "git",
    "url": "https://github.com/youngjuning/juejin-me.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@luozhu/eslint-config-typescript": "^1.8.2",
    "@luozhu/prettier-config": "^1.2.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.2",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "lint-staged": "^11.1.2",
    "mocha": "^8.4.0",
    "prettier": "^2.4.0",
    "typescript": "^4.4.3",
    "vscode-test": "^1.5.2",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e -V"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{md,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
